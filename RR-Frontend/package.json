{
  "name": "radio-report-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker": "node --max_old_space_size=18432 ./node_modules/@angular/cli/bin/ng build --configuration=docker --build-optimizer --output-hashing=all",
    "docker-wroc-test": "ng build --configuration=docker-wroc-test",
    "docker-wroc-develop": "ng build --configuration=docker-wroc-develop",
    "docker-wroc-staging": "ng build --configuration=docker-wroc-staging",
    "test": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e-vs": "npx cypress open",
    "e2e": "npx cypress run --config video=false screenshotOnRunFailure=false",
    "start-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --prod",
    "start-dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "start-test-automation": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --configuration=test-automation",
    "cypress-open-berlin": "npx cypress open --config-file=cypress/config/rrtestneoq.config.json",
    "build-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "prod-build-dev": "build -- --prod=true --base-href=/Frontend/ --build-optimizer --output-hashing=all",
    "lint-ts": "npx tslint --project .",
    "cap:copy:all": "npx cap copy",
    "cap:copy:electron": "npx cap copy electron",
    "cap:copy:android": "npx cap copy android",
    "cap:copy:ios": "npx cap copy ios",
    "cap:serve": "npx cap serve",
    "open:electron": "npx cap open electron",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "cap:sync:all": "npx cap sync",
    "cy:run": "cypress run --config-file cypress/config/rr-staging.config.json --browser chromium --record --key dd54aef8-9f8a-4968-b78e-779527693d8f",
    "cy:parallel": "cypress-parallel -s cy:run -t 5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --parser typescript --print-width 140 --single-quote",
      "tslint --fix --project ."
    ],
    "*.html": ["prettier --write --parser html"],
    "*.json": ["prettier --write --parser json"],
    "*.scss": ["prettier --write --parser scss"],
    "*.css": ["prettier --write --parser css"]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@capacitor/android": "2.4.4",
    "@capacitor/cli": "2.4.4",
    "@capacitor/core": "2.4.4",
    "@capacitor/ios": "2.4.4",
    "@fullcalendar/core": "^5.9.0",
    "@kolkov/angular-editor": "^1.1.4",
    "@microsoft/signalr": "3.1.7",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ajv": "^6.12.6",
    "ajv-i18n": "^3.6.0",
    "all": "0.0.0",
    "automapper-ts": "^1.9.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^3.5.0",
    "core-js": "^2.5.4",
    "ensure-error": "^3.0.1",
    "is-electron": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "ng-circle-progress": "^1.6.0",
    "ng2-pdf-viewer": "^6.0.2",
    "ngx-electron": "^2.2.0",
    "ngx-image-cropper": "^3.3.2",
    "ngx-pagination": "^4.1.0",
    "ngx-toastr": "^11.3.2",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "9.1.3",
    "puppeteer": "^5.1.0",
    "quill": "^1.3.7",
    "rxjs": "^6.5.4",
    "serialize-error": "^7.0.1",
    "ts-simple-nameof": "^1.3.0",
    "tslib": "^1.10.0",
    "uuid": "^7.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^8.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-shadow-dom": "^1.3.0",
    "electron": "11.0.3",
    "husky": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.7.5"
  }
}
